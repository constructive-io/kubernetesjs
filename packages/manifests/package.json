{
  "name": "@kubernetesjs/manifests",
  "version": "0.0.2",
  "author": "Constructive <developers@constructive.io>",
  "description": "Curated operator manifests for KubernetesJS ops",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/constructive-io/kubernetesjs",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/constructive-io/kubernetesjs"
  },
  "bugs": {
    "url": "https://github.com/constructive-io/kubernetesjs/issues"
  },
  "scripts": {
    "clean": "makage clean",
    "prepack": "npm run build",
    "build": "makage build",
    "build:dev": "makage build --dev",
    "lint": "eslint . --fix",
    "pull": "ts-node scripts/pull-manifests.ts",
    "pull:all": "ts-node scripts/pull-manifests.ts --all",
    "test": "jest",
    "test:watch": "jest --watch",
    "tests:unit": "jest --selectProjects unit",
    "codegen": "ts-node scripts/codegen-operators.ts"
  },
  "dependencies": {
    "@kubernetesjs/ops": "workspace:^",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.25.0",
    "@babel/types": "^7.25.0",
    "@types/babel__generator": "^7.6.8",
    "@types/js-yaml": "^4.0.5",
    "makage": "^0.1.8"
  }
}
